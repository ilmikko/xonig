[
        {
                "id":"static",
                "require":"./pool/static.js",
                "match":"^text/.*$",
		"dir":["static"]
        },
        {
                "id":"dynamic",
                "require":"./pool/dynamic.js",
                "match":"^text/node$",
                "dir":["dynamic"]
        },
	{
		"id":"binary",
		"require":"./pool/binary.js",
		"match":"^(image/.*|audio/.*|video/.*)$",
		"dir":["binary"]
	}
]
