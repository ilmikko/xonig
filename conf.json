{
        "port":2222,
        "mime":{
                "file_paths":["./mime.types","/etc/nginx/mime.types"],
                "default":"application/octet-stream"
        },
        "pools":[
                {"id":"template","match":"text/template","dir":"./template"},
                {"id":"dynamic","match":"text/node","dir":"./dynamic"},
                {"id":"static","match":"text/.*","dir":"./static"},
                {"id":"physical","match":".*","dir":"./physical"}
        ]
}
